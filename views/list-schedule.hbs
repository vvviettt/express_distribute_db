<link href="https://unpkg.com/tabulator-tables@5.5.2/dist/css/tabulator.min.css" rel="stylesheet">
{{>sidebarNomal}}
<div class="dashboard">
  <div class="header"></div>
  <div class="container">
    <div class="title-container">
        <div class="title">Danh sách lịch họp</div>
    </div>
    <div id="user-table" class="user-table">
        <div class="tb-header">
            <div class="header-wrapper  w-1">
                <p class="header-text">STT</p>
            </div>
             <div class="header-wrapper  w-2">
                <p class="header-text">Tên</p>
            </div>
            <div class="header-wrapper  w-2">
                <p class="header-text">Email</p>
            </div>
            <div class="header-wrapper  w-1">
                <p class="header-text">Đơn vị</p>
            </div>
             <div class="header-wrapper  w-2">
                <p class="header-text">Chi tiết</p>
            </div>
            <div class="header-wrapper  w-2">
                <p class="header-text">Trạng thái</p>
            </div>
            <div class="header-wrapper  w-2">
                <p class="header-text">Action</p>
            </div>
        </div>
        {{#each requests}}
            <div class="row {{#di @index 2 }}row-highlight{{/di}}">
                <div class="item  w-1">
                    <p class="header-text">{{inc @index}}</p>
                </div>
                <div class="item  w-2">
                    <p class="header-text">{{this/name}}</p>
                </div>
                <div class="item  w-2">
                    <p class="header-text">{{this/email}}</p>
                </div>
                <div class="item  w-1">
                    <p class="header-text">{{this/unitId}}</p>
                </div>
                <div class="item  w-2">
                    <p class="header-text">{{this/desc}}</p>
                </div>
                <div class="item  w-2">
                   <input type="checkbox" data-id="{{this/id}}" id="completed-checkbox" {{#if_eq this/isCompleted true}}checked{{/if_eq}} />
                </div>
                <div class="header-wrapper  w-2">
                    <a href="/dashboard/request/detail?id={{this/id}}">
                        <img style="height: 25px;" src="/images/edit.svg" alt="" srcset="">
                    </a>
                </div>
            </div>
        {{/each}}
    </div>
  </div>
  <div class="container">
    <div class="title-container">
        <div class="title">Danh sách lịch dạy học</div>
    </div>
    <div id="user-table" class="user-table">
        <div class="tb-header">
            <div class="header-wrapper  w-1">
                <p class="header-text">STT</p>
            </div>
             <div class="header-wrapper  w-2">
                <p class="header-text">Tên</p>
            </div>
            <div class="header-wrapper  w-2">
                <p class="header-text">Email</p>
            </div>
            <div class="header-wrapper  w-1">
                <p class="header-text">Đơn vị</p>
            </div>
             <div class="header-wrapper  w-2">
                <p class="header-text">Chi tiết</p>
            </div>
            <div class="header-wrapper  w-2">
                <p class="header-text">Trạng thái</p>
            </div>
            <div class="header-wrapper  w-2">
                <p class="header-text">Action</p>
            </div>
        </div>
        {{#each requests}}
            <div class="row {{#di @index 2 }}row-highlight{{/di}}">
                <div class="item  w-1">
                    <p class="header-text">{{inc @index}}</p>
                </div>
                <div class="item  w-2">
                    <p class="header-text">{{this/name}}</p>
                </div>
                <div class="item  w-2">
                    <p class="header-text">{{this/email}}</p>
                </div>
                <div class="item  w-1">
                    <p class="header-text">{{this/unitId}}</p>
                </div>
                <div class="item  w-2">
                    <p class="header-text">{{this/desc}}</p>
                </div>
                <div class="item  w-2">
                   <input type="checkbox" data-id="{{this/id}}" id="completed-checkbox" {{#if_eq this/isCompleted true}}checked{{/if_eq}} />
                </div>
                <div class="header-wrapper  w-2">
                    <a href="/dashboard/request/detail?id={{this/id}}">
                        <img style="height: 25px;" src="/images/edit.svg" alt="" srcset="">
                    </a>
                </div>
            </div>
        {{/each}}
    </div>
  </div>
</div>
<script>
</script>
<script type="text/javascript" src="https://unpkg.com/tabulator-tables@5.5.2/dist/js/tabulator.min.js"></script>
<script src="/js/request.js"></script>